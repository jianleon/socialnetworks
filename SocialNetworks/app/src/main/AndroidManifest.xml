<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kogi.socialnetworks">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.instagram.instagramapi.InstagramAppClientId"
            android:value="b09363abd21c4ca8836f1b9a76fc8f6e" />

        <meta-data
            android:name="com.instagram.instagramapi.InstagramAppRedirectURL"
            android:value="http://www.kogimobile.com/android/socialfeed/redirect" />

    </application>

</manifest>